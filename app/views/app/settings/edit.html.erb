<div class="card">
  <div class="card-header">
    <h3>Configurations</h3>
  </div>
  <div class="card-body">
    <%= form_for @setting, html: { multipart: true }, url: app_settings_path  do |f| %>
      <div class="form-group">
        <%= f.label :app_name, "Nom de l'application" %>
        <%= f.text_field :app_name, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :devise, "Devise" %>
        <%= f.text_field :devise, class: "form-control" %>
      </div>
      
     
      <div class="row">
        <div class="col-12 text-right">
        <%= f.submit 'Mettre Ã  jour',class: "btn btn-primary btn-sm" %>
        </div>
      </div>
    <% end %>
    
  </div>
</div>
<br>


<% content_for :page_name, 'view.settings' %>

<% content_for :script do %>
  <%= javascript_include_tag asset_url "/templates/adminLte/dist/js/settings.js" %>
<% end %>